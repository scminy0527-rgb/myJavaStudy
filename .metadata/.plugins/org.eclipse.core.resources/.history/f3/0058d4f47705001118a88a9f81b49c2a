//	   datas = '.';
//	   System.out.print(datas);
//	   
//	   numbers = 1;
//	   System.out.print(numbers);
//	   
//	   numbers = 5;
//	   System.out.print(numbers);
//	   System.out.print("\n");
//	   
//	   
//	   
////	   false 출력
