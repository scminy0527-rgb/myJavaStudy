package formatTask;

public class FormatTask02 {
	public static void main(String[] args) {
//		수학점수 국어점수 영어점수
		String name = null;
		int math = 0;
		int korean = 0;
		int english = 0;
		int totalScore = 0;
		
		name = "이규학";
		math = 20;
		korean = 40;
		english = 15;
		totalScore = math + korean + english;
		
		
		
		
	}
}
